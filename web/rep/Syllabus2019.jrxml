	<queryString>
		<![CDATA[SELECT distinct
    SY."ANIO" AS SYLABUS_DOCENTE_ANIO,
    SY."CODIGO_MATERIA" AS SYLABUS_DOCENTE_CODIGO_MATERIA,
    SY."CODIGO_NIVEL" AS SYLABUS_DOCENTE_CODIGO_NIVEL,
    SY."NUMERO_CLASES" AS SYLABUS_DOCENTE_NUMERO_CLASES,
    SY."NUMERO_CREDITOS" AS SYLABUS_DOCENTE_NUMERO_CREDITOS,
    SY."MODALIDAD" AS SYLABUS_DOCENTE_MODALIDAD,
    SY."HORARIO" AS SYLABUS_DOCENTE_HORARIO,
    SY."DESCRIPCION_MATERIA" AS SYLABUS_DOCENTE_DESCRIPCION_MATERIA,
    SY."OBJETIVO_GENERAL" AS SYLABUS_DOCENTE_OBJETIVO_GENERAL,
    SY."OBJETIVO_ESPECIFICO" AS SYLABUS_DOCENTE_OBJETIVO_ESPECIFICO,
    SY."PROCESO_DOCENTE" AS SYLABUS_DOCENTE_PROCESO_DOCENTE,
    SY."EVALUACION" AS SYLABUS_DOCENTE_EVALUACION,
    SY."BIBLIOGRAFIA" AS SYLABUS_DOCENTE_BIBLIOGRAFIA,
    REPLACE(SY."OBSERVACIONES",'&','&amp;') AS SYLABUS_DOCENTE_OBSERVACIONES,
    SY."ESTADO_SYLABUS" AS SYLABUS_DOCENTE_ESTADO_SYLABUS,
    SY."ESTADO_ENVIO" AS SYLABUS_DOCENTE_ESTADO_ENVIO,
    SY."APROBADO_POR" AS SYLABUS_DOCENTE_APROBADO_POR,
    SY."FECHA_APROBACION" AS SYLABUS_DOCENTE_FECHA_APROBACION,
    SY."FECHA_CREA" AS SYLABUS_DOCENTE_FECHA_CREA,
    SY."ARTIC_AMBI_CURRICULAR" AS ARTIC_AMBI_CURRICULAR,
    SY."ARTIC_CAMP_FORMACION" AS ARTIC_CAMP_FORMACION,
    SY."ARTIC_UNID_CURRICULAR" AS ARTIC_UNID_CURRICULAR,
    SY."ARTIC_OTRAS_CARAC" AS ARTIC_OTRAS_CARAC,
    SY."EVALUACION_ELEMENTOS" AS EVALUACION_ELEMENTOS,
    SY."OBSERVACIONES_UTIL_REC" AS OBSERVACIONES_UTIL_REC,
    SY."PROCESO_DOCENTE_ACOMP" AS PROCESO_DOCENTE_ACOMP,
    SY."PROCESO_DOCENTE_ACTIV" AS PROCESO_DOCENTE_ACTIV,
    SY.COA_HORAS_PRACTICAS,
    SY.COA_TRABAJO_AUTONOMO,
    SY.CCD_SEGUIMIENTO_TUTORIA,
    SY.LUGAR_ATENCION,
    VCP.NOMBRE_MATERIA AS V_PENSUM_MATERIA_NOMBRE_MATERIA,
    VCP.AREA,
    VCP.PROGRAMA AS PROGRAMA,
    VCP.IDENTIF_MATERIA AS V_PENSUM_MATERIA_IDENTIF_MATERIA,
    VCP.NOMBRE_PARALELO AS NOMBRE_PARALELO,
    VCP.TRIMESTRE AS V_PENSUM_MATERIA_TRIMESTRE,
    VCP.REGSILABO,
CONCAT(VCP.APELLIDO_COORDINADOR, ' ', VCP.NOMBRE_COORDINADOR) NOMBRE_COORDINADOR,
VCP.EMAIL_COORDINADOR,
(SELECT distinct CONCAT(NOMBRE_COMPLETO,';')  AS [data()] FROM interfaseOcu.dbo.V_SECRETARIA WHERE ARE_CODIGO = VCP.CODIGO_FACULTAD and PRG_CODIGO = VCP.CODIGO_ESP AND ANIO_ACAD = VCP.ANIO For XML PATH (''))  SECRETARIA,
(SELECT distinct CONCAT(EMAIL,';')  AS [data()] FROM interfaseOcu.dbo.V_SECRETARIA WHERE ARE_CODIGO = VCP.CODIGO_FACULTAD and PRG_CODIGO = VCP.CODIGO_ESP For XML PATH (''))  EMAIL_AREA ,
 interfaseOcu.dbo.f_getDocentesSilabo(SY.ANIO,SY.CICLO,SY.CODIGO_MATERIA,SY.CODIGO_NIVEL,SY.COD_PARALELO,SY.COD_PROGRAMA,'E') AS V_PENSUM_MATERIA_EMAIL_PROFESOR,
    interfaseOcu.dbo.f_getDocentesSilabo(SY.ANIO,SY.CICLO,SY.CODIGO_MATERIA,SY.CODIGO_NIVEL,SY.COD_PARALELO,SY.COD_PROGRAMA,'N') as V_PENSUM_MATERIA_NOMBRE_PROFESOR
FROM interfaseOcu.dbo.SYLABUS_DOCENTE SY  WITH (NOLOCK)
INNER JOIN interfaseOcu.dbo.VISTA_COORDINADOR_PROGRAMA VCP ON
  (SY.COD_PROGRAMA =  VCP.CODIGO_ESP and SY.ANIO = VCP.ANIO AND SY.CICLO = VCP.CICLO
  AND SY.CODIGO_MATERIA = VCP.CODIGO_MATERIA AND SY.VAC_CODNUM =  VCP.VAC_CODNUM
  AND SY.CODIGO_PROFESOR = VCP.CODIGO_PROFESOR AND SY.CODIGO_NIVEL = VCP.CODIGO_NIVEL
  AND  SY.COD_PARALELO = VCP.COD_PARALELO   )
WHERE SY.ANIO = $P{anio}     and SY.CICLO = 1     and SY.CODIGO_NIVEL = $P{per}     and SY.CODIGO_MATERIA = $P{asig}
     AND SY.COD_PARALELO = $P{par} and SY.COD_PROGRAMA = $P{prog}]]>
	</queryString>
